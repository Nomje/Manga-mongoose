
 <%- include('../partials/header') %>

 <table id="list">
  <thead>
    <tr>
      <th>Title</th>
      <th></th>
  </thead>
  <tbody>
    <% mangas.forEach(function(m) { %>
      <tr>
        <td><%= m.title %></td>
        <td><img src ="<%= m.picture %>"></td>
        <td><a href ="/mangas/<%=m.id%>">Info</a></td>
        <% }); %>
  </tbody>
 </table>
 <%- include('../partials/footer') %>
 